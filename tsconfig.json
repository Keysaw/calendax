{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ESNext",
		"jsx": "react-jsx",
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"noEmit": false
	},
	"exclude": [
		"node_modules"
	]
}
